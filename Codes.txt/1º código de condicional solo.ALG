Algoritmo "Compras"
//Pseucódigo condicional para compras
//7º pseudocódigo
Var
  x: real


Inicio

   escreval ("Sobrou quantos reais de troco?")
   leia (x)
   
   se (x<10)entao
     escreval ("Então compre leite")
   fimse
   
   se (x>=10) entao
     escreval("Então compre ovo")
   fimse
   
   se (x<5) entao
     escreval ("Então compre pão")
   fimse

   

   
   


Fimalgoritmo